#include<stdio.h>
int main()
{
    float a = 15;
    int b = 5;
    int sum=a+b;
    printf("Summation = %d\n",sum);
    int sub=a-b;
    printf("Subtraction = %d\n",sub);
    int mul=a*b;
    printf("multiplication = %d\n",mul);
    float div=a/b;
    printf("Division=%f",div);
   

    return 0;
}
